<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
   <servlet>
  	<servlet-name>ScriptExecutorServlet</servlet-name>
  	<display-name>ScriptExecutorServlet</display-name>
  	<description></description>
  	<servlet-class>com.kashyap1113.mysqlsynctool.ScriptExecutorServlet</servlet-class>
  </servlet>
   <servlet>
   	<servlet-name>database_requests</servlet-name>
   	<display-name>database_requests</display-name>
   	<description></description>
   	<servlet-class>mysql.database_requests</servlet-class>
   </servlet>
  <servlet-mapping>
  	<servlet-name>ScriptExecutorServlet</servlet-name>
  	<url-pattern>/scriptexecutor.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>database_requests</servlet-name>
  	<url-pattern>/database_requests</url-pattern>
  </servlet-mapping>
</web-app>
